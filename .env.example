# OpenF1 API Configuration
VITE_OPENF1_API_BASE_URL=https://api.openf1.org/v1
VITE_OPENF1_API_KEY=your_api_key_here

# Jolpica API Configuration
VITE_JOLPICA_API_BASE_URL=https://api.jolpica.com/api
VITE_JOLPICA_API_KEY=your_api_key_here

# Application Configuration
VITE_APP_TITLE=McLaren Racing Analytics
VITE_APP_ENV=development

# Feature Flags
VITE_ENABLE_PREDICTIVE_ANALYTICS=true
VITE_ENABLE_AI_SUMMARY=true
VITE_ENABLE_STRATEGY_SCORING=true

# Data Cache
VITE_CACHE_DURATION_MINUTES=30
VITE_MAX_HISTORICAL_SEASONS=10
